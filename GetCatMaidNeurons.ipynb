      " |  \n",
      " |  image_stacks\n",
      " |      Image stacks available under this project id.\n",
      " |  \n",
      " |  user_permissions\n",
      " |      List per-project permissions and groups of user with given token.\n",
      " |  \n",
      " |  ----------------------------------------------------------------------\n",
